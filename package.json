{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.8.4",
    "crypto-browserify": "^3.12.1",
    "dotenv": "^16.4.7",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "eject": "react-scripts eject",
    "build:css": "postcss build/**/*.css --replace",
    "build:html": "posthtml src/**/*.tsx -o src/tsx/tsx",
    "watch:css": "postcss src/**/*.css --watch --dir src/css/css",
    "watch:html": "posthtml src/**/*.html -o dist --watch",
    "process:tsx": "ts-node posthtml.config.ts",
    "build:all": "concurrently \"npm run build:css\" \"npm run process:tsx\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
    "@esbuild-plugins/node-modules-polyfill": "^0.2.2",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.14",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^9.1.2",
    "cssnano": "^7.0.6",
    "fs-extra": "^11.3.0",
    "mini-css-extract-plugin": "^2.9.2",
    "postcss": "^8.5.3",
    "postcss-class-prefix": "^0.3.0",
    "postcss-cli": "^11.0.1",
    "postcss-loader": "^8.1.1",
    "postcss-prefix-selector": "^2.1.0",
    "posthtml": "^0.16.6",
    "posthtml-cli": "^0.10.0",
    "posthtml-parser": "^0.12.1",
    "posthtml-render": "^3.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2",
    "vite": "^6.2.4",
    "vite-plugin-node-polyfills": "^0.23.0",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1"
  },
  "description": "This project was bootstrapped with [Create React App](https://github.com/facebook/create-react-app).",
  "main": "postcss.config.js",
  "keywords": [],
  "author": "",
  "license": "ISC"
}
